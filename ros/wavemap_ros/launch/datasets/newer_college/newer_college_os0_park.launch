<launch>
  <!-- Dataset params -->
  <arg name="only_first_rosbag"
       default="false"
       doc="If true, replay only the first the rosbag in the dataset. If false, replay everything."/>

  <!-- Select the rosbag paths -->
  <!-- NOTE: Relative paths will be resolved w.r.t. ROS_HOME (default ~/.ros) -->
  <arg if="$(arg only_first_rosbag)" name="rosbag_path"
       value="
../data/wavemap_experiments/newer_college/dataset/2021-ouster-os0-128-alphasense/collection2/park/2021-11-30-17-09-49_0-park.bag"/>
  <arg unless="$(arg only_first_rosbag)" name="rosbag_path" value="
../data/wavemap_experiments/newer_college/dataset/2021-ouster-os0-128-alphasense/collection2/park/2021-11-30-17-09-49_0-park.bag
../data/wavemap_experiments/newer_college/dataset/2021-ouster-os0-128-alphasense/collection2/park/2021-11-30-17-13-13_1-park.bag
../data/wavemap_experiments/newer_college/dataset/2021-ouster-os0-128-alphasense/collection2/park/2021-11-30-17-16-38_2-park.bag
../data/wavemap_experiments/newer_college/dataset/2021-ouster-os0-128-alphasense/collection2/park/2021-11-30-17-20-07_3-park.bag
../data/wavemap_experiments/newer_college/dataset/2021-ouster-os0-128-alphasense/collection2/park/2021-11-30-17-23-25_4-park.bag
../data/wavemap_experiments/newer_college/dataset/2021-ouster-os0-128-alphasense/collection2/park/2021-11-30-17-26-36_5-park.bag
../data/wavemap_experiments/newer_college/dataset/2021-ouster-os0-128-alphasense/collection2/park/2021-11-30-17-30-06_6-park.bag
../data/wavemap_experiments/newer_college/dataset/2021-ouster-os0-128-alphasense/collection2/park/2021-11-30-17-33-19_7-park.bag"/>

  <!-- Launch default Newer College OS0 setup -->
  <include
      file="$(find wavemap_ros)/launch/datasets/newer_college/newer_college_os0_base.launch"
      pass_all_args="true"/>
</launch>
